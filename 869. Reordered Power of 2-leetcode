class Solution(object):
    def reorderedPowerOf2(self, x):
        """
        :type n: int
        :rtype: bool
        """
        def pow2(x):
            return ((x & (x-1)) == 0)
        lis = []
        while(x!=0):
            lis.append(x%10)
            x/=10
        per = itertools.permutations(lis,len(lis))
        per = [p for p in per if p[0]!=0]
        for i in per:
            k = int(''.join(map(str,i)))
            if(pow2(k)):
                return True
        return False
