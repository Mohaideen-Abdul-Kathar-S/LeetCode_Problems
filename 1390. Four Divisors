class Solution {
    public int sumFourDivisors(int[] nums) {
        int sum = 0;
        for(int i:nums){
            int cnt = 0;
            int temp = 0;
            for(int j = 1; j*j<=i && cnt!=5;j++){
                if(i%j==0){
                    int k = i/j;
                    if(k==j){
                    cnt++;
                    temp+=i;
                    }else{
                        cnt++;
                        cnt++;
                        temp += j + k;
                    }
                }
                if(cnt>4) break;
            }
            if(cnt==4) sum += temp;
           
                
        }
        return sum;


    }
}

