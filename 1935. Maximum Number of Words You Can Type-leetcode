class Solution {
    public int canBeTypedWords(String text, String br) {
        String[] str = text.split(" ");
        int count=0;
        for(String s : str){
            boolean check = true;
            for(char c : s.toCharArray()){
                if( br.indexOf(c)!=-1){
                    check = false;
                    break;
                }
            }
            if(check){
                count++;
            }
        }
        return count;
    }
}
