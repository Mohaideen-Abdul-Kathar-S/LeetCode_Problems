class Solution {
    public List<Integer> majorityElement(int[] nums) {
        int ele1=Integer.MIN_VALUE, ele2=Integer.MIN_VALUE, c1=0, c2=0, n = nums.length;
        for(int i=0;i<n;i++){
            if(c1==0 && ele2 != nums[i]){
                ele1 = nums[i];
                c1++;
            }
            else if(c2==0 && ele1 != nums[i]){
                ele2 = nums[i];
                c2++;
            }
            else if(ele1==nums[i]) c1++;
            else if(ele2==nums[i]) c2++;
            else{
                c1--;
                c2--;
            }
        }
        int minCount = (int)n/3 +1;
   
        c1=0;
        c2=0;
        List<Integer> ls = new ArrayList<Integer>();
        for(int i=0;i<n;i++){
            if(ele1==nums[i]) c1++;
            else if(ele2 == nums[i]) c2++;
        }
        if(c1>=minCount) ls.add(ele1);
        if(c2>=minCount) ls.add(ele2);
      
        Collections.sort(ls);
        return ls;
    }
}
