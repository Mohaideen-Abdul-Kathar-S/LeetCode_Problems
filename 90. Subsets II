class Solution {
    void subset(int[] nums,int i,int n,List<List<Integer>> ls, List<Integer> ans){
        if(i==n){
            if(!ls.contains(ans)){
                ls.add(new ArrayList<>(ans));
        }
            return;
        }
        ans.add(nums[i]);
        subset(nums,i+1,n,ls,ans);
        ans.remove(ans.size()-1);
        subset(nums,i+1,n,ls,ans);
    }
    public List<List<Integer>> subsetsWithDup(int[] nums) {
        Arrays.sort(nums);
        List<List<Integer>> ls = new ArrayList<>();
        List<Integer> ans = new ArrayList<>();
        subset(nums,0,nums.length,ls, ans);
        return ls;
    }
}
