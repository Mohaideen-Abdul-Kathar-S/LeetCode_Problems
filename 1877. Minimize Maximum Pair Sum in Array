class Solution {
    public int minPairSum(int[] nums) {
        Arrays.sort(nums);
        int n = nums.length;
        int num = -1;
        for(int i = 0;i<n/2;i++){
            if(nums[i]+nums[n-1-i]>num){
                num = nums[i]+nums[n-1-i];
            }
        }
        return num;
    }
}
