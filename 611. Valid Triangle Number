class Solution(object):
    def triangleNumber(self, num):
        """
        :type nums: List[int]
        :rtype: int
        """
        c=0
        num = sorted(num)
        for i in range(len(num)-2):
            for k in range(i+1,len(num)-1):
                for j in range(k+1,len(num)):
                

                    if(num[i]+num[k]>num[j] and num[i]+num[j]>num[k] and num[j]+num[k]>num[i]):
                        
                        c=c+1
        return c
