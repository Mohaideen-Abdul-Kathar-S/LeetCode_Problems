class Solution {
    public int minimumBoxes(int[] apple, int[] capacity) {
        Arrays.sort(capacity);
      
        int cnt =0, sum=0;
        for(int i:apple){
            sum+=i;
        }
        for(int i=capacity.length-1;i>=0;i--){
            cnt++;
            if(capacity[i]>=sum){

                return cnt;
            }
            sum -= capacity[i];
        }
        return cnt;
    }
}
