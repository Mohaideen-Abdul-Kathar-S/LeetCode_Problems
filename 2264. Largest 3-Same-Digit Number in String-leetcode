class Solution(object):
    def largestGoodInteger(self, num):
        """
        :type num: str
        :rtype: str
        """
        c = ''
        for i in range(0,len(num)-2):
            if(num[i]==num[i+1]==num[i+2]):
                if(c=='' or c[0]<num[i]):
                    c = num[i]*3
        return c
